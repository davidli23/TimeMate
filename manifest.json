{
	"name": "TimeMate",
	"version": "1.1",
	"description": "Website blocker to help keep you productive.",
	"content_scripts": [{
    	"matches": ["<all_urls>"],
    	"js": ["content.js"]
  	}],
  	"icons": {
  		"128": "assets/logo128.png",
  		"48": "assets/logo48.png"
  	},
  	"background": {
  		"scripts": ["background.js"]
  	},
	"browser_action": {
		"default_icon": "assets/logo48.png",
       	"default_popup": "popup.html"
	},
	"manifest_version": 2
}
